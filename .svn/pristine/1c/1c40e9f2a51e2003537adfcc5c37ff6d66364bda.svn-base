.main .main_tit h3 {
  height: 40px;
  line-height: 40px;
  background: #f3f3f3;
  color: #874604;
  border-bottom: 1px solid #c8c8c8;
  border-top: 1px solid #c8c8c8;
  font-size: 15px;
  padding-left: 10px;
}
.main .main_tit h3 a {
  color: #874604;
}
/* 10大品牌 */
.ajaxTop li {
  background: url(../images/arrow-right.svg) no-repeat right center / 20px;
  height: 57px;
  padding: 10px 0;
  margin-right: 10px;
  border-bottom: 1px solid #e7e7e7;
}
.ajaxTop li a {
  display: block;
}
.ajaxTop li b {
  float: left;
  width: 20px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  display: block;
  background: #c9c9c9;
  color: #fff;
  font-weight: normal;
  margin: 0 12px 0 10px;
}
.ajaxTop li div {
  overflow: hidden;
  font-size: 16px;
  line-height: 20px;
}
.ajaxTop li div .count {
  font-size: 12px;
  color: #999;
}
.ajaxTop li:nth-child(1) b {
  background: #f10e0e;
}
.ajaxTop li:nth-child(2) b {
  background: #ff9314;
}
.ajaxTop li:nth-child(3) b {
  background: #8adf5b;
}
/* 品牌销量 */
.ajaxSell li {
  position: relative;
  padding: 10px;
  border-bottom: 1px solid #e7e7e7;
}
.ajaxSell li a {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 8;
}
.ajaxSell .brandImg {
  float: left;
  width: 120px;
  height: 120px;
  margin-right: 10px;
}
.ajaxSell .brandImg img {
  width: 120px;
  height: 120px;
}
.ajaxSell .con {
  overflow: hidden;
  font-size: 15px;
}
.ajaxSell .con .title {
  line-height: 28px;
  height: 56px;
  overflow: hidden;
  font-family: Tahoma, sans-serif;
}
.ajaxSell .con .price {
  margin: 8px 0;
  color: #ed0000;
  font-size: 18px;
}
.ajaxSell .con .price span {
  font-size: 13px;
}
.ajaxSell .con .other {
  color: #a1a1a1;
  font-size: 12px;
}
.ajaxSell .con .star {
  position: absolute;
  right: 10px;
  top: 70px;
  font-size: 20px;
  color: #ff841d;
  word-spacing: 25px;
}
/* 产品评论 */
.ajaxComm {
  padding-bottom: 18px;
}
.ajaxComm .good {
  padding: 10px;
}
.ajaxComm .brandImg {
  float: left;
  margin-right: 12px;
}
.ajaxComm .brandImg img {
  width: 80px;
  height: 80px;
}
.ajaxComm > .con {
  overflow: hidden;
  line-height: 28px;
  height: 56px;
  font-size: 15px;
  font-family: Tahoma, '微软雅黑', sans-serif;
}
.ajaxComm .comm {
  border: 1px solid #fff;
  margin: 0 10px;
  padding: 10px;
  border-radius: 5px;
  background: #F8F8F8;
  font-size: 15px;
  margin-bottom: 15px;
  box-shadow: 0 0 5px #ccc;
}
.ajaxComm .comm_title {
  position: relative;
  font-size: 14px;
  height: 20px;
  line-height: 20px;
  margin-bottom: 10px;
  color: #4f6900;
}
.ajaxComm .comm_title .date {
  position: absolute;
  right: 10px;
  top: 4px;
  color: #999;
}
.ajaxComm .comm_title .star {
  margin: 0 8px;
  font-size: 17px;
  color: #ff841d;
}
.ajaxComm .comm_con {
  line-height: 22px;
}
